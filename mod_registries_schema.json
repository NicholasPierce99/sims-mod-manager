{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "https://example.com/sims_registry_schema.schema.json",
    "title": "Registry For Sims Mods",
    "description": "This document records the details of a place to download sims mods from, usually a website",
    "type": "object",
    "properties": {
        "id": {
            "description": "A unique identifier for a registry",
            "type": "number"
        },
        "name": {
            "description": "Name of the registry",
            "type": "string"
        },
        "link": {
            "description": "Link to the registry",
            "type": "string"
        },
        "metadata": {
            "description": "Metadata of the registry",
            "type": "object",
            "properties": {
                "download_link_element": {
                    "type": "string"
                },
                "most_recent_registered_date": {
                    "type": "string",
                    "format": "date-time"
                }
            }
        }
    }
}